"""
This script computes the fiber flat field correction from a DESI continuum lamp frame.
"""

import sys
import desispec.scripts.autocalib_fiberflat as autocalib_fiberflat

if __name__ == '__main__':
    args = autocalib_fiberflat.parse()
    sys.exit(autocalib_fiberflat.main(args))
